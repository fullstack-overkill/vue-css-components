
.add-btn
    display: flex
    align-items: center
    justify-content: center
    user-select none
    cursor pointer
    color: black
    background-color: #03DAC5
    border none 
    border-radius: 50%
    width: 60px
    height 60px
    position: absolute
    right 35px
    bottom: 35px
    -webkit-box-shadow: 0px 0px 83px 200px rgba(0,0,0,0.75);
    -moz-box-shadow: 0px 0px 83px 200px rgba(0,0,0,0.75);
    box-shadow: 0px 0px 43px 2px rgba(0,0,0,0.25);


.add-btn:focus
    outline none
    border: none                           

.card-paper
    display: flex
    flex-direction: column
    padding: 16px
    border-radius: 5px
    margin-top: 15px
    width 49%
    box-shadow: 0px 2px 1px -1px rgba(0, 0, 0, 0.2), 
    0px 1px 1px 0px rgba(0, 0, 0, 0.14), 
    0px 1px 3px 0px rgba(0,0,0,.12)
    .card-paper-title
        font-size: 25px
        color: rgba(0,0,0,1)

.container-test
    display: flex
    justify-content: center
    justify-items: center
    width: 100%
    height 800px
    .app-phone
        display: flex
        width: 520px
        height: 800px
        position absolute
        box-shadow: -2px 2px 5px 1px rgba(0,0,0,0.6)
        .page-analytics
            display flex
            flex-direction: column
            flex: 1
            .analytics-header
                background-color: #6200EE
                padding-left: 14px
                padding-right: 14px
                display: flex
                flex-direction: row
                justify-content: space-between
                .header-menu
                    display: flex
                    align-items: center
                    height 66px
                    .menu-opp
                        display: flex
                        align-items: center
                        justify-content: center
                        color: #ffffff
                        cursor pointer
                        user-select none
                        background-color: rgba(0,0,0,0)
                        border: none 
                    .menu-opp:focus
                        outline none
                        background-color: rgba(0,0,0,0)
                        border: none     
                    .menu-text
                        font-family: 'Roboto', sans-serif;
                        font-weight: 300
                        font-size: 20px
                        font-weight: 500
                        color: #ffffff    
               .menu-opp-secondary
                    display: flex
                    .menu-search
                        display: flex
                        align-items: center
                        justify-content: center
                        color: #ffffff
                        cursor: pointer
                        border: none 
                        cursor pointer
                        user-select none
                        background:rgba(0,0,0,0)           
                    .menu-search:focus
                        outline none
                        background-color: rgba(0,0,0,0)
                        border: none 
                     .menu-more
                        display: flex
                        align-items: center
                        justify-content: center
                        color: #ffffff
                        cursor: pointer
                        border: none 
                        cursor pointer
                        user-select none
                        background:rgba(0,0,0,0)           
                    .menu-more:focus
                        outline none
                        background-color: rgba(0,0,0,0)
                        border: none        
                        
            .analytics-body   
                width: 100%         
                display: flex
                flex-direction: row
                flex-wrap: wrap
                flex: 1
                padding 22px
                justify-content: space-between
                